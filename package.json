{
  "name": "dora-plugin-atool-build",
  "version": "0.2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/dora-js/dora-plugin-atool-build"
  },
  "homepage": "https://github.com/dora-js/dora-plugin-atool-build",
  "author": "chencheng <sorrycc@gmail.com>",
  "license": "MIT",
  "main": "./lib",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore __tests__",
    "lint": "eslint --ext .js src"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "koa-webpack-dev-middleware": "~1.0.1"
  },
  "devDependencies": {
    "babel-cli": "~6.2.0",
    "babel-core": "~6.1.21",
    "babel-plugin-add-module-exports": "~0.1.1",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-stage-0": "~6.1.18",
    "eslint": "~1.9.0",
    "eslint-config-airbnb": "~1.0.0",
    "pre-commit": "~1.1.2"
  },
  "peerDependencies": {
    "atool-build": "0.x",
    "dora": "0.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ]
}
